<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!--分割线-->
    <style name="LineStyle">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">0.3dp</item>
        <item name="android:background">@color/line</item>
    </style>

    <style name="BottomInAndOutStyle">
        <item name="android:imeExtractEnterAnimation">@anim/in_bottom</item>
        <item name="android:imeExtractExitAnimation">@anim/out_bottom</item>
    </style>
</resources>